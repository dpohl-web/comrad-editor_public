{
  "name": "questionnaire-form",
  "version": "1.1.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --verbose",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "typedoc": "typedoc --mode modules -module commonjs --exclude **/*.spec.ts --out docs/typedoc src/app",
    "copybuildfiles": "./copyfiles.sh",
    "buildforwp": "ng build --configuration production --output-hashing none",
    "buildforwpandcopy": "ng build --configuration production --base-href /surveycreator/ && npm run copybuildfiles",
    "test:playwright": "ng e2e --playwright-config=playwright.config.ts",
    "test:finish": "ng test --include=src/app/footer/footer.component.spec.ts",
    "builddev": "ng build --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/material": "^18.2.14",
    "@angular/material-moment-adapter": "^18.2.14",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "@tinymce/tinymce-angular": "^8.0.1",
    "core-js": "^3.40.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "ngx-color-picker": "^17.0.0",
    "ngx-ui-loader": "^13.0.0",
    "nouislider": "^15.8.1",
    "rxjs": "~7.8.1",
    "tinymce": "^7.6.1",
    "tslib": "^2.6.1",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.14",
    "@angular/cli": "^18.2.14",
    "@angular/compiler-cli": "^18.2.13",
    "@angular/language-service": "^18.2.13",
    "@playwright/test": "^1.50.1",
    "@types/jasmine": "~3.8.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "source-map-explorer": "^1.6.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typedoc": "^0.15.5",
    "typescript": "~5.4.5"
  },
  "engines": {
    "node": ">=20.16.0 <22",
    "npm": "~10.8.1"
  }
}
